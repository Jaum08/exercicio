#include <stdio.h>

int main() {
    int numero, i;

    printf("Digite um número para ver sua tabela de multiplicação: ");
    scanf("%d", &numero);

    printf("Tabela de multiplicação do %d:\n", numero);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
